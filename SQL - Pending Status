select
date(handover_time) handover_date
,station_name
,flag
,order_category
,buyer_region
,whs_id
,o_type
,HOUR(sla_start_ontime) as COT_Arrived
,COUNT(distinct shipment_id ) as vol
from dev_vnbi_spx.soc_kpi_ontime_outdate
where station_name in ('HCM Mega SOC','Xuyen A SOC','BW SOC','SW SOC')
-- and date(handover_time)
group by 1,2,3,4,5,6,7,8
order by 1 desc;
